{
  "AllowedHosts": "*",
  "Serilog": {
    "using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "Minimumlevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "Path": "C:\\Logs\\ShivShellLog.log",
          "rollingIinterval": "Day"
        }
      }
    ]
  }

}
